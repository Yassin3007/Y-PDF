{
    "name": "yassin/ypdf",
    "description": "Y-PDF: Headless browser PDF generator for Laravel supporting full HTML/CSS/JS stacks.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/view": "^10.0"
    },
    "suggest": {
        "chrome-php/chrome": "Required when using the built-in Chrome driver."
    },
    "autoload": {
        "psr-4": {
            "YPdf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YPdf\\YPdfServiceProvider"
            ],
            "aliases": {
                "YPdf": "YPdf\\Facades\\YPdf"
            }
        }
    }
}
